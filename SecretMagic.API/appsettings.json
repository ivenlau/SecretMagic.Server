{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MySqlDatabase": "<YourMySqlInstance>;Database=<YourMySqlDb>;User=<YourMySqlUser>;Password=<YourMySqlPassword>;"
  },
  "TokenConfig": {
    "secret": "<YourSecret>",
    "issuer": "www.secretmagic.tech",
    "audience": "api",
    "accessExpiration": 30,
    "refreshExpiration": 60
  },
  "OssConfig": {
    "region": "cn-hangzhou",
    "ossRegion": "oss-cn-hangzhou",
    "stsEndPoint": "sts.cn-hangzhou.aliyuncs.com",
    "ossEndPoint": "oss-cn-hangzhou.aliyuncs.com",
    "accessKeyId": "<YourAccessKeyId>",
    "accessKeySecret": "<YourAccessKeySecret>",
    "bucket": "<YourBucket>",
    "expiration": 30,
    "roleArn": "<YourRoleArn>",
    "roleSessionName": "OssAccessSession"
  }
}
